{
  "name": "legends-of-runaterra-ui",
  "version": "0.0.0",
  "description": "nodecg parce template",
  "homepage": "https://github.com/Hectorban/Nodecg-parcel-template",
  "author": "Alysanne <hectorban1234@gmail.com> (http://alysanne.dev/)",
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "src",
    "extension"
  ],
  "scripts": {
    "start": "start npm run watchDashboard && start npm run watchGraphics",
    "watchDashboard": "parcel watch src/dashboard/*.html --out-dir dashboard --public-url ./",
    "watchGraphics": "parcel watch src/graphics/*.html --out-dir graphics --public-url ./",
    "buildDashboard": "parcel build src/dashboard/*.html --out-dir dashboard --public-url ./",
    "buildGraphics": "parcel build src/graphics/*.html --out-dir graphics --public-url ./",
    "build": "start npm run buildDashboard && start npm run buildGraphics"
  },
  "keywords": [
    "template",
    "react",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "panel1",
        "title": "Panel1",
        "width": 3,
        "file": "panel.html",
        "headerColor": "#525F78",
        "workspace": "Legends of Runeterra"
      },
      {
        "name": "panel2",
        "title": "Panel2",
        "width": 2,
        "file": "panel2.html",
        "headerColor": "#525F78",
        "workspace": "Legends of Runeterra"
      },
      {
        "name": "panel3",
        "title": "Panel3",
        "width": 2,
        "file": "panel3.html",
        "headerColor": "#525F78",
        "workspace": "Legends of Runeterra"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "index2.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "index3.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "2.2.1",
    "sass": "^1.32.11"
  },
  "dependencies": {
    "flux": "^4.0.1",
    "node-fetch": "^2.6.1",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.13.8",
    "runeterra": "^3.0.0",
    "svg-path-properties": "^1.0.11",
    "text-to-svg": "^3.1.5"
  }
}
